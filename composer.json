{
  "name": "railroad/railforums",
  "description": "Laravel forum system using entities",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/dbal": "^3.3",
    "ezyang/htmlpurifier": "^4.10",
    "railroad/railnotifications": "2.0.*",
    "railroad/resora": "^2.0",
    "railroad/permissions": "^2.0",
    "symfony/dom-crawler": "^6.0",
    "staudenmeir/laravel-upsert": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "3.6.*@dev",
    "orchestra/database": "3.6.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railforums\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
